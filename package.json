{
  "name": "kofo-wallet",
  "version": "1.2.3",
  "description": "Kofo network supports blockchain wallet, which provides signature and private key management",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha test/**/*.test.js",
    "compile": "rm -rf lib && babel src -d lib",
    "build": "npm run compile && rm -rf dist/ && webpack --config webpack.config.js",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drunken005/kofo-wallet.git"
  },
  "keywords": [
    "wallet",
    "blockchain",
    "kofo",
    "zil",
    "eos",
    "btc",
    "tron",
    "bos",
    "meetone",
    "eth",
    "bos",
    "hpb",
    "bnb"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/drunken005/kofo-wallet/issues"
  },
  "homepage": "https://github.com/drunken005/kofo-wallet#readme",
  "dependencies": {
    "@babel/runtime": "^7.4.4",
    "@zilliqa-js/zilliqa": "^0.5.3",
    "binance-utils": "^1.0.2",
    "bip32": "^2.0.3",
    "bitcoinjs-lib": "^5.0.5",
    "bitcore-lib": "^8.3.3",
    "eosjs-ecc": "^4.0.4",
    "ethers": "^4.0.27",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-async-to-generator": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "assert": "^2.0.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^23.6.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-stage-2": "^6.24.1",
    "browserify": "^16.2.3",
    "browserify-zlib": "^0.2.0",
    "buffer": "^5.2.1",
    "mocha": "^6.1.4",
    "qunit": "^2.9.2",
    "should": "^13.2.3",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-async-to-generator",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-runtime"
    ]
  },
  "files": [
    "lib/*",
    "!lib/webpack.build.js"
  ]
